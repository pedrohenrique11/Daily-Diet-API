generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String
  password String
  meals Meal[]
}
model Meal {
  id Int @id @default(autoincrement())
  title String
  description String?
  onDiet Boolean @default(true)
  date String
  user User @relation(fields: [userId], references: [id])
  userId Int
}